<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="custom_design_update" sortOrder="60">
        <field name="country_fieldset" formElement="select" >
<!--               class="Test\CmsBlocks\Ui\Component\Form\Country">-->
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string" translate="true">Country</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Country</label>
                <dataScope>country_fieldset</dataScope>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Test\CmsBlocks\Ui\Component\Form\Country"/>
                        <caption translate="true">-- Please Select --</caption>
                    </settings>
                </select>
            </formElements>
        </field>
    </fieldset>
</form>
